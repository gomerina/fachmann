    <script src="//api-maps.yandex.ru/2.1/?&lang=ru-RU&onload=getYaMap"></script>
    <script src="js/main.js"></script>
    <script>
        function getYaMap() {
            var myMap = new ymaps.Map('map1', {
                center: [54.585304, 39.775227],
                zoom: 14
            }, {
                searchControlProvider: 'yandex#search'
            }),
                MyIconContentLayout = ymaps.templateLayoutFactory.createClass(
                    '<div style="color: #FFFFFF; font-weight: bold;">$[properties.iconContent]</div>'
                ),
                myPlacemark = new ymaps.Placemark(myMap.getCenter(), {

                }, {
                    iconLayout: 'default#image',
                    iconImageHref: 'img/svg/marker.svg',
                    iconImageSize: [25, 33],
                    iconImageOffset: [-5, -38]
                })
            myMap.geoObjects.add(myPlacemark)
            myMap.behaviors.disable('scrollZoom')


            var myMap2 = new ymaps.Map('map2', {
                center: [54.955189, 82.852661],
                zoom: 14
            }, {
                searchControlProvider: 'yandex#search'
            }),
                MyIconContentLayout2 = ymaps.templateLayoutFactory.createClass(
                    '<div style="color: #FFFFFF; font-weight: bold;">$[properties.iconContent]</div>'
                ),
                myPlacemark2 = new ymaps.Placemark(myMap2.getCenter(), {

                }, {
                    iconLayout: 'default#image',
                    iconImageHref: 'img/svg/marker.svg',
                    iconImageSize: [25, 33],
                    iconImageOffset: [-5, -38]
                })
            myMap2.geoObjects.add(myPlacemark2)
            myMap2.behaviors.disable('scrollZoom')


            var myMap3 = new ymaps.Map('map3', {
                center: [45.153203, 38.992300],
                zoom: 14
            }, {
                searchControlProvider: 'yandex#search'
            }),
                MyIconContentLayout3 = ymaps.templateLayoutFactory.createClass(
                    '<div style="color: #FFFFFF; font-weight: bold;">$[properties.iconContent]</div>'
                ),
                myPlacemark3 = new ymaps.Placemark(myMap3.getCenter(), {

                }, {
                    iconLayout: 'default#image',
                    iconImageHref: 'img/svg/marker.svg',
                    iconImageSize: [25, 33],
                    iconImageOffset: [-5, -38]
                })
            myMap3.geoObjects.add(myPlacemark3)
            myMap3.behaviors.disable('scrollZoom')
        }
    </script>
